<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/profilemain"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#243642"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="410dp"
        android:layout_height="928dp"
        android:background="@drawable/roundedcornerlightgreen"
        android:orientation="vertical"
        android:paddingRight="15dp"
        android:paddingBottom="15dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <LinearLayout
            android:layout_width="84dp"
            android:layout_height="40dp"
            android:layout_marginRight="10dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imgarrow"
                android:layout_width="94dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="209dp"
                android:layout_marginRight="20dp"
                android:layout_marginBottom="802dp"
                app:srcCompat="@drawable/arrow" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingRight="15dp">

            <ImageButton
                android:id="@+id/imgprofile"
                android:layout_width="377dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:background="@drawable/roundedcornerlightgreen"
                android:paddingLeft="30dp"
                app:srcCompat="@drawable/profile" />
        </LinearLayout>

        <TextView
            android:id="@+id/txtname"
            android:layout_width="340dp"
            android:layout_height="83dp"
            android:layout_gravity="center"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/roundedcornergrassgrees"
            android:drawableStart="@drawable/blackstar"
            android:elevation="1dp"
            android:gravity="center"
            android:text="Profile 1"
            android:textColor="#FFFFFF"
            android:textSize="34sp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="67dp"
                android:layout_marginStart="20dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="20dp"
                android:layout_marginRight="30dp"
                android:layout_marginBottom="15dp"
                android:orientation="horizontal"
                android:paddingLeft="20dp"
                android:paddingRight="20dp">

                <ImageButton
                    android:id="@+id/btncall"
                    android:layout_width="88dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="30dp"
                    android:layout_weight="1"
                    android:background="@drawable/roundedcornergrassgrees"
                    app:srcCompat="@drawable/phone" />

                <ImageButton
                    android:id="@+id/btnmessage"
                    android:layout_width="89dp"
                    android:layout_height="match_parent"
                    android:layout_marginRight="17dp"
                    android:layout_weight="1"
                    android:background="@drawable/roundedcornergrassgrees"
                    app:srcCompat="@drawable/text" />

                <ImageButton
                    android:id="@+id/btnvidcall"
                    android:layout_width="92dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/roundedcornergrassgrees"
                    app:srcCompat="@drawable/vidcam" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="354dp"
                android:layout_height="234dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="25dp"
                android:layout_marginRight="20dp"
                android:background="@drawable/roundedcornergrassgrees"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="4dp"
                    android:layout_marginTop="4dp"
                    android:layout_marginRight="4dp"
                    android:layout_marginBottom="4dp"
                    android:orientation="vertical"
                    android:paddingLeft="10dp"
                    android:paddingTop="10dp"
                    android:paddingRight="10dp">

                    <TextView
                        android:id="@+id/lblcontact1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Contact 1"
                        android:textColor="#FFFFFF"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/lblcontactno1"
                        android:layout_width="match_parent"
                        android:layout_height="43dp"
                        android:paddingLeft="15dp"
                        android:paddingTop="4dp"
                        android:text="+639214232581"
                        android:textColor="#FFFFFF"
                        android:textSize="30sp"
                        android:textStyle="normal" />

                    <TextView
                        android:id="@+id/borderdonttouch"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp"
                        android:text="-------------------------------------------"
                        android:textColor="#FFFFFF"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/lblcontact2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Contact 2"
                        android:textColor="#FFFFFF"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/lblcontactno"
                        android:layout_width="match_parent"
                        android:layout_height="43dp"
                        android:paddingLeft="15dp"
                        android:paddingTop="4dp"
                        android:text="+639564232234"
                        android:textColor="#FFFFFF"
                        android:textSize="30sp"
                        android:textStyle="normal" />

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="25dp"
                android:orientation="horizontal"
                android:paddingLeft="10dp"
                android:paddingTop="25dp">

                <Button
                    android:id="@+id/btncancel"
                    android:layout_width="144dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="25dp"
                    android:layout_marginRight="10dp"
                    android:backgroundTint="#E2F1E7"
                    android:text="Delete"
                    android:textColor="#000000"
                    android:textSize="25sp"
                    android:textStyle="normal"
                    android:visibility="visible" />

                <Button
                    android:id="@+id/btnsave"
                    android:layout_width="187dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="25dp"
                    android:layout_marginRight="10dp"
                    android:layout_weight="1"
                    android:backgroundTint="#E2F1E7"
                    android:paddingLeft="55dp"
                    android:text="Edit"
                    android:textColor="#000000"
                    android:textSize="25sp"
                    android:textStyle="normal"
                    android:visibility="visible" />

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>